eval {
  set low 1
  set patchlevel [string trimleft $vim_patchlevel 0]
  while {$low <= $patchlevel} {
    set high [expr $low + 99];
    if {$high < $patchlevel} {
      patchfiles-append \
        [format "%s.%03d-%03d.gz" $vim_version $low $high]
      incr low 100
    } else {
      patchfiles-append \
        [format "%s.%03d" $vim_version $low]
      incr low 1
    }
  }
}

checksums-append \
