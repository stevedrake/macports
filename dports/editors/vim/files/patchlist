eval {
  set low 1
  set patchlevel [string trimleft $vim_patchlevel 0]
  set files {}

  while {$low <= $patchlevel} {
    set high [expr $low + 99];
    # temporary workaround: don't fetch 101-200 gzipped, due to 126ne
    # if {$high < $patchlevel} {}
    if {$high < 199} {
      lappend files \
        [format "%s.%03d-%03d.gz" $vim_version $low $high]
      incr low 100
    } else {
      lappend files \
        [format "%s.%03d" $vim_version $low]
      incr low 1
    }
  }

  # replace patchfiles superseeded by others
  foreach f $files {
    if {$f == "7.1.126"} {
      set f "7.1.126ne"
    }
    patchfiles-append $f
  }
}

