# $Id: Portfile,v 1.7 2006/07/15 10:46:43 rhwood Exp $
PortSystem 1.0
name		gnome2-user-docs
version		2.8.1
revision	1
description	OBSOLETE. Use gnome-user-docs port instead.
long_description \
	This package has been replaced by the gnome-user-docs package. \
\
	Please uninstall this package prior to installing the gnome-user-docs \
	package. The change in packages was done to better align the \
	Darwinports GNOME distrobution with the upstream package set for GNOME.
maintainers	macports-dev@lists.macosforge.org
categories	gnome
platforms	darwin 
homepage	http://www.gnome.org/
fetch		{
	ui_msg	""
	ui_msg	"Port gnome2-user-docs has been superseded by port gnome-user-docs."
	ui_msg	"gnome-user-docs will be installed after removing gnome2-user-docs."
	ui_msg	""
}
checksum	{}
configure	{}
build		{}
destroot	{}
install		{
	ui_msg	"Removing gnome2-user-docs"
	system	"port -f uninstall gnome2-user-docs"
	ui_msg	"Installing gnome-user-docs"
	system	"port install gnome-user-docs"
	ui_msg	"Port gnome-user-docs has been installed."
}
activate	{}
